{% extends 'base.html.twig' %}

{% block title %}Register{% endblock %}

{% block body %}
<div class="container">
  <div class="title text-center">
      <h1> POPULAR MOVIES </h1>
  </div>
  <div class="row my-3">
    {% for popular in populars %}
        <div class="my-3 text-center col-4 ">
            <img src="http://image.tmdb.org/t/p/w342{{popular.poster_path}}" alt="poster_{{popular.title}}">
            <h5>{{popular.title}}</h5>
            <h5>popularity: {{popular.popularity}}</h5>
      </div>
      {% endfor %}
  </div>

</div>







    <div class="">


    </div>
{% endblock %}
